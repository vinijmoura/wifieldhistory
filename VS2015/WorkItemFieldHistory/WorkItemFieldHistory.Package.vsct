    </GuidSymbol>
        
    <GuidSymbol name="guidCmdIcon" value="{CA44E2BD-CA21-4FDD-8EAA-2E421A811701}">
      <IDSymbol name="bmpCmdIcon" value="1" />
    </GuidSymbol>

    <!-- This is the Work Item Explorer Menu. -->
